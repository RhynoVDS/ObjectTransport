<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <RootNamespace>OTransport</RootNamespace>
    <PackageId>ObjectTransport</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>Rhyno van der Sluijs</Authors>
    <Description>A simple library that allows you to send and receive Objects over TCP</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright></Copyright>
    <PackageTags>TCP Server Object networking</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.Threading.Thread" Version="4.3.0" />
  </ItemGroup>

</Project>
